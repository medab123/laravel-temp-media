{
  "name": "elamed123/laravel-temp-media",
  "description": "Laravel package for handling temporary media uploads with automatic cleanup",
  "keywords": [
    "laravel",
    "package",
    "media",
    "upload",
    "temporary",
    "spatie",
    "media-library"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Elabidi Mohammed",
      "email": "mohammed.elabidi123@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "spatie/laravel-medialibrary": "^10.0|^11.0"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "Medox\\LaravelTempMedia\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Medox\\LaravelTempMedia\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Medox\\LaravelTempMedia\\TempMediaServiceProvider"
      ],
      "aliases": {
        "TempMedia": "Medox\\LaravelTempMedia\\Facades\\TempMedia"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
